{% extends 'base.html.twig' %}

{% block page_title %}
    <h1>{{ 'Forgot password'|trans }}</h1>
{% endblock %}

{% block body %}
    {{ form_start(form)}}
    {{ form_errors(form)}}
    {{ form_row(form.email)}}
    {{ form_rest(form)}}

    <button type="submit">{{ 'Submit'|trans }}</button>

    {{ form_end(form)}}
{% endblock %}